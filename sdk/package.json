{
  "name": "@opentuna/sdk",
  "version": "3.1.0",
  "description": "OpenTuna Autonomous Agent SDK - Full primitives for email, slack, discord, whatsapp, cron, MCP, google workspace, notion, and trading automation on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/opentuna.mjs",
      "require": "./dist/opentuna.js",
      "types": "./dist/opentuna.d.ts"
    },
    "./legacy": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./trading-agent": {
      "import": "./dist/trading-agent.mjs",
      "require": "./dist/trading-agent.js",
      "types": "./dist/trading-agent.d.ts"
    },
    "./subtuna": {
      "import": "./dist/subtuna.mjs",
      "require": "./dist/subtuna.js",
      "types": "./dist/subtuna.d.ts"
    },
    "./email": {
      "import": "./dist/email.mjs",
      "require": "./dist/email.js",
      "types": "./dist/email.d.ts"
    },
    "./slack": {
      "import": "./dist/slack.mjs",
      "require": "./dist/slack.js",
      "types": "./dist/slack.d.ts"
    },
    "./discord": {
      "import": "./dist/discord.mjs",
      "require": "./dist/discord.js",
      "types": "./dist/discord.d.ts"
    },
    "./whatsapp": {
      "import": "./dist/whatsapp.mjs",
      "require": "./dist/whatsapp.js",
      "types": "./dist/whatsapp.d.ts"
    },
    "./cron": {
      "import": "./dist/cron.mjs",
      "require": "./dist/cron.js",
      "types": "./dist/cron.d.ts"
    },
    "./mcp": {
      "import": "./dist/mcp.mjs",
      "require": "./dist/mcp.js",
      "types": "./dist/mcp.d.ts"
    },
    "./google": {
      "import": "./dist/google.mjs",
      "require": "./dist/google.js",
      "types": "./dist/google.d.ts"
    },
    "./notion": {
      "import": "./dist/notion.mjs",
      "require": "./dist/notion.js",
      "types": "./dist/notion.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenClawTrade/opentuna-sdk.git"
  },
  "keywords": [
    "opentuna",
    "autonomous-agents",
    "solana",
    "ai-agents",
    "trading-agents",
    "email-automation",
    "slack-integration",
    "discord-bot",
    "whatsapp-api",
    "cron-scheduling",
    "mcp-protocol",
    "google-workspace",
    "notion-api",
    "launchpad",
    "defi",
    "agent-sdk",
    "colosseum-hackathon"
  ],
  "author": "OpenTuna Team <team@tuna.fun>",
  "license": "MIT",
  "homepage": "https://tuna.fun/opentuna",
  "bugs": {
    "url": "https://github.com/OpenClawTrade/opentuna-sdk/issues"
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "test": "jest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
